        <!--Skype-->
        <application id="@+id/pcapp_skype" name="Skype" procname="skype.exe">
            <hotbtn id="@id/hotbtn_vol_up" shortcut="Up"/>
            <hotbtn id="@id/hotbtn_vol_down" shortcut="Down"/>
            <hotkey id="@+id/hotkey_contacts" shortcut="Alt+1" label="contacts"/>
            <hotkey id="@+id/hotkey_recent" shortcut="Alt+2" label="recent"/>
        </application>

        <!--VLC-->
        <application id="@+id/pcapp_vlc" name="VLC" procname="vlc.exe">
            <hotbtn id="@id/hotbtn_vol_up" shortcut="Ctrl+Up"/>
            <hotbtn id="@id/hotbtn_vol_down" shortcut="Ctrl+Down"/>
