<?xml version="1.0" encoding="utf-8"?>
<Apps>
    <os name="windows">

        <!--Windows-->
        <hotkey id="@+id/oskey_alttab" shortcut="Alt+Tab"/>
        <hotkey id="@+id/oskey_switch_window_start" shortcut="$Win+Tab+$100"/>
        <hotkey id="@+id/oskey_switch_window_next" shortcut="Tab"/>
        <hotkey id="@+id/oskey_switch_window_prev" shortcut="Shift+Tab"/>
        <hotkey id="@+id/oskey_switch_window_finish" shortcut="Win$"/>
        <!--<hotkey id="@+id/oskey_switch_window_start" shortcut="$Alt+Tab+$100"/>
        <hotkey id="@+id/oskey_switch_window_next" shortcut="Tab"/>
        <hotkey id="@+id/oskey_switch_window_prev" shortcut="Shift+Tab"/>
        <hotkey id="@+id/oskey_switch_window_finish" shortcut="Alt$"/>-->

        <command id="@+id/oscommand_lock_computer" text="rundll32.exe user32.dll, LockWorkStation "/>

        <!--EXPLORER.EXE-->
        <application id="@+id/pcapp_explorer" name="Windows" procname="EXPLORER.EXE">
        </application>

        <!--IDEA-->
        <application id="@+id/pcapp_intellij_idea" name="IntelliJ IDEA" procname="idea.exe">
            <hotbtn id="@id/hotbtn_vol_up" shortcut="Ctrl+Alt+Right"/>
            <hotbtn id="@id/hotbtn_vol_down" shortcut="Ctrl+Alt+Left"/>
            <hotkey id="@+id/hotkey_run" shortcut="Shift+F10" label="run"/>
            <hotkey id="@+id/hotkey_debug" shortcut="Shift+F9" label="debug"/>
            <hotkey id="@+id/hotkey_goto_file" shortcut="Ctrl+Shift+N" label="to file"/>
            <hotkey id="@+id/hotkey_cvs_commit" shortcut="Ctrl+K" label="commit"/>
            <hotkey id="@+id/hotkey_cvs_update" shortcut="Ctrl+T" label="update"/>
            <hotkey id="@+id/hotkey_project_structure" shortcut="Ctrl+Alt+Shift+S" label="project\nstructure"/>
            <hotkey id="@+id/hotkey_structure_popup" shortcut="Ctrl+F12" label="structure\npopup"/>
            <hotkey id="@+id/hotkey_find_usages" shortcut="Alt+F7" label="find\nusages"/>
            <hotkey id="@+id/hotkey_hide_show_windows" shortcut="Ctrl+Shift+F12" label="hide/show\nwindows"/>
            <hotkey id="@+id/hotkey_close" shortcut="Ctrl+F4" label="close"/>
        </application>

        <!--Eclipse-->
        <application id="@+id/pcapp_eclipse" name="Eclipse" procname="eclipse.exe">
            <hotkey id="@+id/hotkey_run" shortcut="Ctrl+F11" label="run"/>
            <hotkey id="@+id/hotkey_debug" shortcut="F11" label="debug"/>
            <hotkey id="@+id/hotkey_open_resource" shortcut="Ctrl+Shift+R" label="open resource"/>
            <hotkey id="@+id/hotkey_open_type" shortcut="Ctrl+Shift+T" label="open type"/>
            <hotkey id="@+id/hotkey_quick_outline" shortcut="Ctrl+O" label="quick outline"/>
            <hotkey id="@+id/hotkey_to_editors" shortcut="Ctrl+E" label="to editors"/>
            <hotkey id="@+id/hotkey_close" shortcut="Ctrl+F4" label="close"/>
        </application>

        <!--Chrome-->
        <application id="@+id/pcapp_google_chrome" name="Google Chrome" procname="chrome.exe">
            <hotbtn id="@id/hotbtn_vol_up" shortcut="Ctrl+Tab"/>
            <hotbtn id="@id/hotbtn_vol_down" shortcut="Ctrl+Shift+Tab"/>
            <hotkey id="@+id/hotkey_address_bar" shortcut="Ctrl+L" label="address\nbar"/>
            <hotkey id="@+id/hotkey_prev_tab" shortcut="Ctrl+Shift+Tab" label="previous\ntab"/>
            <hotkey id="@+id/hotkey_next_tab" shortcut="Ctrl+Tab" label="next\ntab"/>
            <hotkey id="@+id/hotkey_new_tab" shortcut="Ctrl+T" label="new\ntab"/>
            <hotkey id="@+id/hotkey_close_tab" shortcut="Ctrl+F4" label="close\ntab"/>
            <hotkey id="@+id/hotkey_open_last_closed" shortcut="Ctrl+Shift+T" label="open\nclosed tab"/>
            <hotkey id="@+id/hotkey_history" shortcut="Ctrl+H" label="history"/>
            <hotkey id="@+id/hotkey_downloads" shortcut="Ctrl+J" label="downloads"/>
            <hotkey id="@+id/hotkey_to_favorites" shortcut="Ctrl+D" label="to favorites"/>
        </application>

        <!--Firefox-->
        <application id="@+id/pcapp_firefox" name="Mozilla Firefox" procname="firefox.exe">
            <hotbtn id="@id/hotbtn_vol_up" shortcut="Ctrl+Tab"/>
            <hotbtn id="@id/hotbtn_vol_down" shortcut="Ctrl+Shift+Tab"/>
            <hotkey id="@+id/hotkey_address_bar" shortcut="Ctrl+L" label="address\nbar"/>
            <hotkey id="@+id/hotkey_prev_tab" shortcut="Ctrl+Shift+Tab" label="previous\ntab"/>
            <hotkey id="@+id/hotkey_next_tab" shortcut="Ctrl+Tab" label="next\ntab"/>
            <hotkey id="@+id/hotkey_new_tab" shortcut="Ctrl+T" label="new\ntab"/>
            <hotkey id="@+id/hotkey_close_tab" shortcut="Ctrl+F4" label="close\ntab"/>
            <hotkey id="@+id/hotkey_history" shortcut="Ctrl+H" label="history"/>
            <hotkey id="@+id/hotkey_downloads" shortcut="Ctrl+J" label="downloads"/>
            <hotkey id="@+id/hotkey_to_favorites" shortcut="Ctrl+D" label="to favorites"/>
            <hotkey id="@+id/hotkey_open_favorites" shortcut="Ctrl+I" label="open\nfavorites"/>
        </application>

        <!--IE-->
        <application id="@+id/pcapp_ie" name="Internet Explorer" procname="iexplore.exe">
            <hotbtn id="@id/hotbtn_vol_up" shortcut="Ctrl+Tab"/>
            <hotbtn id="@id/hotbtn_vol_down" shortcut="Ctrl+Shift+Tab"/>
            <hotkey id="@+id/hotkey_address_bar" shortcut="Ctrl+L" label="address\nbar"/>
            <hotkey id="@+id/hotkey_prev_tab" shortcut="Ctrl+Shift+Tab" label="previous\ntab"/>
