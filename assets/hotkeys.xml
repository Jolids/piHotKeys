<?xml version="1.0" encoding="utf-8"?>
<Apps>
    <os name="windows">

        <!--Windows-->
        <hotkey id="alttab" shortcut="Alt+Tab"/>
        <hotkey id="wintab" shortcut="$Win+Tab+$100"/>
        <hotkey id="tab" shortcut="Tab"/>
        <hotkey id="shifttab" shortcut="Shift+Tab"/>
        <hotkey id="win" shortcut="Win$"/>
        <!--<hotkey  shortcut="$Alt+Tab+$100"/>
        <hotkey  shortcut="Tab"/>
        <hotkey  shortcut="Shift+Tab"/>
        <hotkey  shortcut="Alt$"/>-->

        <command text="rundll32.exe user32.dll, LockWorkStation "/>

